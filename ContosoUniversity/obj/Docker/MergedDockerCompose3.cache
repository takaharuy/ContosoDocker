services:
  contosodb:
    container_name: ContosoSqlDb
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: pass%w0rd
    image: mcr.microsoft.com/mssql/server:2019-latest
    ports:
    - published: 1433
      target: 1433
    restart: always
  contosouniversity:
    build:
      context: C:\Users\t.yuji\source\repos\ContosoDocker\ContosoDocker\ContosoUniversity
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ContosoUniversity
      target: base
    container_name: ContosoUniversity
    depends_on:
      contosodb:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://+:5000
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: contosouniversity:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/fallbackpackages
        --additionalProbingPath /root/.nuget/fallbackpackages2  "/app/bin/Debug/netcoreapp3.1/ContosoUniversity.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8801
      target: 5000
    tty: true
    volumes:
    - C:\Users\t.yuji\source\repos\ContosoDocker\ContosoDocker\ContosoUniversity:/app:rw
    - C:\Users\t.yuji\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\t.yuji\AppData\Roaming\ASP.NET\Http:/root/.aspnet/http:ro
    - C:\Users\t.yuji\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\t.yuji\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
    - C:\Users\t.yuji\source\repos\ContosoDocker\ContosoDocker\ContosoUniversity:/src:rw
version: '3.4'